oading: state.loading,\n      errors: state.errors.user,\n      user: state.user,\n      managedBand: state.managedBand\n  }\n}\n\nexport default connect(readAccess, { clearBandErrors, newBand, editBand })(Login);","/home/mike/flatiron/projects/staged_frontend/src/reducers/manageErrors.js",[],["177","178"],"/home/mike/flatiron/projects/staged_frontend/src/components/SuggestedPost.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageSuggestedEvents.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageUser.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageFollowedEvents.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageManagedBandEvents.js",[],"/home/mike/flatiron/projects/staged_frontend/src/components/MiniBandPost.js",[],"/home/mike/flatiron/projects/staged_frontend/src/containers/BandFeed.js",[],["179","180"],"/home/mike/flatiron/projects/staged_frontend/src/containers/BandShow.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageShowBand.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reportWebVitals.js",[],"/home/mike/flatiron/projects/staged_frontend/src/reducers/manageDateEvents.js",[],"/home/mike/flatiron/projects/staged_frontend/src/API.js",[],{"ruleId":"181","replacedBy":"182"},{"ruleId":"183","replacedBy":"184"},{"ruleId":"181","replacedBy":"185"},{"ruleId":"183","replacedBy":"186"},{"ruleId":"181","replacedBy":"187"},{"ruleId":"183","replacedBy":"188"},{"ruleId":"181","replacedBy":"189"},{"ruleId":"183","replacedBy":"190"},{"ruleId":"181","replacedBy":"191"},{"ruleId":"183","replacedBy":"192"},{"ruleId":"193","severity":1,"message":"194","line":5,"column":8,"nodeType":"195","messageId":"196","endLine":5,"endColumn":17},{"ruleId":"181","replacedBy":"197"},{"ruleId":"183","replacedBy":"198"},{"ruleId":"181","replacedBy":"199"},{"ruleId":"183","replacedBy":"200"},"no-native-reassign",["201"],"no-negated-in-lhs",["202"],["201"],["202"],["201"],["202"],